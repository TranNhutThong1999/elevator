{
  "services": [
    {
      "name": "frontend",
      "rootDir": "frontend",
      "buildCommand": "npm install && npm run build",
      "startCommand": "npx serve -s build",
      "installCommand": "npm install"
    },
    {
      "name": "backend",
      "rootDir": "backend",
      "buildCommand": "npm install && npm run build",
      "startCommand": "node dist/main.js",
      "installCommand": "npm install"
    }
  ]
}
